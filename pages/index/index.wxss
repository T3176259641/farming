page{
  background: #E8FFF5;
}
.parents{
  height: 65rpx;
}
.childs{
  height: 65rpx;
  background:white;
  display: flex;
  justify-items: center;
  border-radius: 35px 35px 35px 35px;
  margin-left: 8px;
  margin-right: 8px;
  box-shadow: 1px 1px 1px 1px rgba(0, 0, 0, .3);
}
.search{
  display: flex;
  justify-items: center;
  align-items: center;
  margin-left: 15rpx;
}
.search_swiper{
  width: 100%;
}
.search_swiper swiper-item{
  display: flex;
  justify-content:center;
}
.tx_tab{
  text-align: center;
  display: flex;
  align-items: center;
  color:#8E8E8E;
  font-size: 25rpx;
}
.btn_parent{
  display: flex;
  justify-content:flex-end;
  align-items: center;
  margin-right: 10rpx;
  border-radius: 35px 35px 35px 35px;
}
.btn_ok{
  display: flex;
  justify-content:center;
  align-items: center;
  background: #ADFEDC;
  width: 110rpx;
  height: 50rpx;
  box-shadow: 1px 1px 1px 1px rgba(0, 0, 0, .2);
  border-radius: 35px 35px 35px 35px;
}
.btn_ok2{
  display: flex;
  align-items: center;
  color: #3C3C3C;
}
.btn_ok2 text{
  font-size: 25rpx;
}
.shopping_parent{
  height: 300rpx;
  background:#F0FFF0;
  border-radius: 14rpx;
  margin-top: 25rpx;
  margin-left: 8px;
  margin-right: 8px;
  box-shadow: 1px 1px 1px 1px rgba(0, 0, 0, .2);
}
.shopping_view1{
  display: flex;
  margin-left: 15rpx;
  flex-wrap: wrap;
  justify-content: space-around;
}
.cont_img{
  width: 110rpx;
  height: 120rpx;
  margin-top: 15rpx;
  margin-bottom: 15rpx;
  display: flex;
  margin-left: 15rpx;
  margin-right: 35rpx;
  justify-content: space-around;
  flex-wrap: wrap;
}
.image_shop{
  display: flex;
  justify-content: space-around;
  text-align: center;
}
.tx_sp{
  text-align: center;
  display: flex;
  font-size: 13px;
  margin-top: 10rpx;
  justify-content: space-around;
}
.seckill_parent{
  height: 220rpx;
  border-radius: 14rpx;
  margin-top: 25rpx;
  margin-left: 8px;
  margin-right: 8px;
}
.seckill_childs{
  display:flex;
  justify-content: space-around;
  align-items: center;
}
.seckill_child1{
  width: 350rpx;
  height: 220rpx;
  background:#F0FFF0;
  border-radius: 15rpx;
  box-shadow: 1px 1px 1px 1px rgba(0, 0, 0, .2);
}
.seckill_title{
  width: 100%;
  height: 50rpx;
  background-color:#DFFFDF;
  border-radius: 15rpx;
  border-bottom: solid rgba(0, 0, 0, .5) 1px;
}
.tiltle{
  text-align: center;
  color:#007500;
  font-weight: bold;
  font-size: 15px;
}
.shopping_Parent_img{
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.shopping_Childs_img{
  margin-top: 20rpx;
}
.shopping_Childs_img > image{
  height: 110rpx; 
  width: 160rpx;
  border-radius: 15rpx;
}
.show_shopping_Parent{
  height: 220rpx;
  border-radius: 14rpx;
  margin-top: 25rpx;
  margin-left: 8px;
  margin-right: 8px;
}
.show_shopping_Childs{
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
}
.show_shopping{
  width: 350rpx;
  height: 500rpx;
  background:white;
  border-radius: 18rpx;
  box-shadow: 1px 1px 1px rgba(0, 0, 0, .7);
}
.show_shopping_iamges{
  width: 100%;
  height: 100%;
  border-radius: 20rpx;
}
.show_shopping_Childs_img{
  width: 100%;
  height: 320rpx;
  border-radius: 20rpx 20rpx 0rpx 0rpx;
  background: yellow;
}
.show_shopping_iamges2{
  width: 100%;
  height: 100%;
  border-radius: 20rpx 20rpx 0rpx 0rpx;
}
.tx_align{
  margin-top: 8rpx;
  margin-left: 8rpx;
  margin-right: 8rpx;
}
.tx_shoppping{
  width: 100%;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.tx_shoppping text{
  font-size: 32rpx;
}
.price{
  margin-top: 10rpx;
}
.text_price_parent{
  margin-left: 8rpx;
}
.text_price1{
  font-size: 15px;
  font-weight: bold;
  color:#AE0000;
}
.text_price2{
  font-size: 20px;
  font-weight: bold;
  color:#AE0000;
}
.comment_parents{
  margin-left: 8rpx;
}
.comment_parents text{
  font-size: 22rpx;
  color:#9D9D9D;
}
.bottom{
  margin-bottom: 25rpx;
}