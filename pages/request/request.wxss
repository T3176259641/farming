page{
    background-color:#FCFCFC;
}
.parents{
    width: 100%;
    height: 950rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}
.childs{
    width: 650rpx;
    height: 660rpx;
    border-radius: 10rpx;
    background:	#F0F0F0;
    box-shadow: 1px 1px 1px 1.5px rgba(0, 0, 0, .2);
}
.margin_style{
    margin-top: 100rpx;
}
.childs_img{
    width: 100%;
    margin: auto;
    justify-content:center;
    flex-wrap: wrap;
    display:flex;
    justify-items: center;
}
.childs_img2{
    width: 150rpx;
    height: 150rpx;
    border-radius: 100%;
    border: 4px solid white;
}
.childs_img image{
    width: 100%;
    height: 100%;
}
.tx{
    margin-top: 20rpx;
    width: 350rpx;
    text-align: center;
    font-size: 28rpx;
    margin-bottom: 20rpx;
}
.tx02{
    margin-top: 10rpx;
    width: 350rpx;
    color: #8E8E8E;
    text-align: center;
    font-size: 21rpx;
}
.select_style{
    margin-top: 60rpx;
    margin-right: 35rpx;
}
.select_sl{
    width: 100%;
    display: flex;
    justify-content:flex-end;
}
.btn_style{
    display: flex;
    justify-content: center;
    align-items: center;
    width: 85rpx;
    height: 85rpx;
    background:	#F0F0F0;
    margin-right: 20rpx;
    border-radius: 100%;
    box-shadow: 1px 1px 1px 1px rgba(0, 0, 0, .3);
}
.btn_style1{
    display: flex;
    margin-left: 15rpx;
    justify-content: center;
    align-items: center;
    width: 250rpx;
    height: 85rpx;
    color:#9D9D9D;
    background:	#F0F0F0;
    border-radius: 25px 25px 25px 25px;
    box-shadow: 1px 1px 1px 1px rgba(0, 0, 0, .3);
}
.btn_style1 > text{
    margin-left: 25rpx;
}