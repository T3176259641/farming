/*类型栏css*/
.shopping_parent{
  height: 820rpx;
  background:#F0FFF0;
  border-radius: 14rpx;
  margin-top: 25rpx;
  margin-left: 8px;
  margin-right: 8px;
}
.cont_img{
  width: 500rpx;
  height: 750rpx;
  margin-top: 15rpx;
  margin-bottom: 15rpx;
  display: flex;
  margin-left: 15rpx;
  margin-right: 35rpx;
  justify-content: center;
  flex-wrap: wrap;
}
.cont_img image{
  width: 100%;
  height: 100%;
  border-radius: 15rpx;
  box-shadow: 5rpx 5rpx 20rpx rgba(0, 0, 0, .2);
}
.image_recommend{
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
}
/* ********************************* */
/* 轮播组件css */
.shopping_swiper{
  margin-top: 30rpx;
  margin-left: 15rpx;
  margin-right: 15rpx;
  border-radius: 15rpx;
  display: flex;
  box-shadow: 1px 1px 1px 1px rgba(0, 0, 0, .2);
}
.swiper_img{
  width: 100%;
  height: 100%;
  border-radius: 15rpx 0 0 15rpx;
}
.vice_parent{
  width: 30%;
  background: #F0FFF0;
  border-radius: 0 15rpx 15rpx 0;
}
.btn_shopping{
  height:22%;
  margin-top: 25rpx;
  display: flex;
  border-radius: 0 0rpx 0rpx 0;
  justify-content: space-around;
  align-items: center;
  box-shadow: 1px 1px 1px 1px rgba(0, 0, 0, .2);
}
.text_swp{
  display: flex;
  color: #4F4F4F;
  justify-content: center;
  align-items: center;
}
/* ****************************************** */
.show_shopping_Parent{
    height: 220rpx;
    border-radius: 14rpx;
    margin-top: 25rpx;
    margin-left: 8px;
    margin-right: 8px;
  }
  .show_shopping_Childs{
    display: flex;
    justify-content: space-around;
    flex-wrap: wrap;
  }
  .show_shopping{
    width: 350rpx;
    height: 500rpx;
    background:white;
    border-radius: 18rpx;
    box-shadow: 1px 1px 1px rgba(0, 0, 0, .7);
  }
  .show_shopping_iamges{
    width: 100%;
    height: 100%;
    border-radius: 20rpx;
  }
  .show_shopping_Childs_img{
    width: 100%;
    height: 320rpx;
    border-radius: 20rpx 20rpx 0rpx 0rpx;
    background: yellow;
  }
  .show_shopping_iamges2{
    width: 100%;
    height: 100%;
    border-radius: 20rpx 20rpx 0rpx 0rpx;
  }
  .tx_align{
    margin-top: 8rpx;
    margin-left: 8rpx;
    margin-right: 8rpx;
  }
  .tx_shoppping{
    width: 100%;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
  }
  .tx_shoppping text{
    font-size: 32rpx;
  }
  .price{
    margin-top: 10rpx;
  }
  .text_price_parent{
    margin-left: 8rpx;
  }
  .text_price1{
    font-size: 15px;
    font-weight: bold;
    color:#AE0000;
  }
  .text_price2{
    font-size: 20px;
    font-weight: bold;
    color:#AE0000;
  }
  .comment_parents{
    margin-left: 8rpx;
  }
  .comment_parents text{
    font-size: 22rpx;
    color:#9D9D9D;
  }
  .bottom{
    margin-bottom: 25rpx;
  }